{
    "name": "roberthansen/mobilepay-php",
    "description": "mobilepay-php",
    "type": "library",
    "homepage": "https://github.com/robert-hansen/mobilepay-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert",
            "email": "robert@monetizemedia.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "illuminate/http": "^9.1",
        "guzzlehttp/guzzle": "^7.2",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "RobertHansen\\MobilePay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RobertHansen\\MobilePay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RobertHansen\\MobilePay\\MobilePayServiceProvider"
            ],
            "aliases": {
                "MobilePay": "RobertHansen\\MobilePay\\Facades\\MobilePay"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
